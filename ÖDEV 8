UPDATE employee
SET name = 'Meacer'
WHERE name ~~ 'M%'
RETURNING *;

UPDATE employee
SET name = 'Bitter'
WHERE name ~~* '%t'
RETURNING *;

UPDATE employee
SET name = 'Andre'
WHERE name ~~ 'A%'
RETURNING *;

UPDATE employee
SET email = 'Jonathan'
WHERE email ~~ '%@johnathan.com'
RETURNING *;

UPDATE employee
SET birthday = '2002-02-21'
WHERE birthday IS NULL
RETURNING *;





DELETE FROM employee
WHERE name = 'Wilma'
RETURNING *;

DELETE FROM employee
WHERE email = 'plumleyz'
RETURNING *;

DELETE FROM employee
WHERE name ~~* '%b%b%'
RETURNING *;

DELETE FROM employee
WHERE name ~~ '_t%'
RETURNING *;

DELETE FROM employee
WHERE email ~~ '%.org'
RETURNING *;

SELECT city.city, country.country FROM city LEFT JOIN country ON city.city_id = country.country_id;
SELECT customer.first_name, customer.last_name, payment.payment.id FROM customer RIGHT JOIN payment ON payment.customer_id = customer.customer_id;
SELECT customer.first_name, customer.last_name, rental.rental_id FROM customer FULL JOIN rental ON customer.customer_id = rental.customer_id;
